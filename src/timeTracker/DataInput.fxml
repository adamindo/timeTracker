<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8"
          fx:controller="timeTracker.Controller">
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="27.0" minHeight="0.0" prefHeight="26.0"/>
        <RowConstraints maxHeight="50.0" minHeight="23.0" prefHeight="24.0"/>
        <RowConstraints maxHeight="50.0" minHeight="23.0" prefHeight="24.0"/>
        <RowConstraints maxHeight="50.0" minHeight="23.0" prefHeight="24.0"/>
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" onAction="#readTextField" text="Button" GridPane.rowIndex="1"/>
        <TextField fx:id="sampleTextField"/>
        <Label fx:id="errorMessage" textFill="#bf0404" wrapText="true" GridPane.rowIndex="2" GridPane.rowSpan="2"/>
    </children>
</GridPane>
